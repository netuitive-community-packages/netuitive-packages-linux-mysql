        "match": "netuitive.linux.mysql.fulltablescans.*",
        "properties": {
          "baseline": true,
          "correlation": true
        }
      },
      {
        "match": "netuitive.linux.mysql.slowqueries.*",
        "properties": {
          "baseline": false,
          "correlation": false
        }
