      },
      {
        "match": "mysql.Opened_.*",
        "properties": {
          "baseline": true,
          "correlation": true
        }
      },
      {
        "match": "mysql.Prepared_stmt_count",
        "properties": {
          "baseline": false,
          "correlation": false
        }
      },
      {
        "match": "mysql.Queries",
        "properties": {
          "baseline": true,
          "correlation": true
